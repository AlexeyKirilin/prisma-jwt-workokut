{
  "trailingComma": "es5",
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "semicolons": true,
  "arrowParens": "avoid",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["THIRD_PARTY_MODULES", "/routes/", "/middleware/", "^../(.*)", "^./(.*)"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
